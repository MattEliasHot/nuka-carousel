{
  "presets": ["env", "react",
    "stage-3",
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 3 versions",
            "ie >= 9",
            "safari >= 8"
          ]
        },
        "useBuiltIns": true,
        "modules": false
      }
    ]
  ],
  "plugins": [
    "add-module-exports", 
    "transform-decorators-legacy",
    "transform-class-properties", 
    "transform-object-rest-spread"
  ]
}
